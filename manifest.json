{
  "manifest_version": 3,
  "name": "City Map Finder",
  "version": "1.0",
  "description": "Find top 5 cities of a country with map links",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}

  